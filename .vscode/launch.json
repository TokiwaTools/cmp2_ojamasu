{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch local file",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceRoot}/index.php"
        },
        {
            "name": "Attach Chrome",
            "type": "chrome",
            "request": "attach",
            "port": 9222,
            "webRoot": "${workspaceRoot}/index.php"
        }
    ]
}